header {
    height: 40px;
    width: 100%;
    background-color: white;
    position: fixed;
}

header input[type=search] {
    width: 100%;
    height: 30px;
    line-height: 30px;
    border-radius: 30px;
    border: 1px solid #c4c7cc;
    margin-top: 5px;
    padding: 10px;
}

body {
    background-color: #DADADA;
}

.head {
    display: -webkit-flex;
    -webkit-flex: auto 2;
}

.wrapper {
    border: 1px solid #e0e0e0;
}

.main-listview {
    background-color: white;
    padding: 10px;
    margin: 10px;
    border-radius: 4px;
}

.main-listview:first-child {
    margin-top: 50px;
}

.list-child {
    margin: 10px 5px;
    display: -webkit-flex;
    -webkit-flex: auto 2;
    font-size: 14px;

}

.list-child label {
    width: 125px;
    color: #aeaeae;
}

.main-listview div:first-child {
    border-bottom: 1px solid #C0C0C0;
    padding-bottom: 10px;
}

ul {
    list-style: none;
    margin: 0;
    padding: 0;
    width: 100%;
}

.search-body {
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: white;
    margin-top: 35px;
    display: none;
    overflow: hidden;
}

.search-body a {
    position: fixed;
    width: 100%;
    height: 40px;
    line-height: 40px;
    background-color: rgb(0, 171, 243);
    bottom: 0px;
    text-align: center;
    color: white;
}

.sift-row {
    padding: 0 4% 10px;
    border-bottom: 1px solid #e7e7e7;
}
.sift-row .row-head {
    line-height: 28px;
    height: 28px;
    margin: 10px 0;
    color: #c4c7cc;
}


.sift-row .title {
    vertical-align: middle;
    font-size: 14px;
    font-family: tahoma,arial,'Hiragino Sans GB','Microsoft Yahei',sans-serif;
    line-height: 28px;
    display: inline-block;
}
.sift-row .selected-items {
    color: rgb(0, 171, 243);
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: middle;
    display: inline-block;
    max-width: 50%;
    font-size: 12px;
    margin-left: 4px;
    line-height: 28px;
}

.sift-row .row-body {
    height: 80px;
    overflow: hidden;
    margin-right: -2%;
}
.sift-row .row-body2 {
    height: 30px;
    overflow: hidden;
    display: -webkit-flex;
    -webkit-flex: auto 2;

}

.sift-row .row-body2 input{
    width: 50%;
    overflow: hidden;
    margin: 0px 10px;

}

.sift-row input  {

    width: 95%;
    height: 25px;
    line-height: 25px;
    border-radius: 5px;
    border: 1px solid #c4c7cc;
    padding: 0px 5px;




}
.sift-item-selected {
    background: rgb(0, 171, 243)!important;
    color: #fff;
}
.sift-item {
    line-height: 35px;
    border-radius: 2px;
    height: 35px;
    width: 30%;
    margin: 0 2% 10px 0;
    text-align: center;
    background: #f2f2f2;
    display: inline-block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.sift-item2 {
    line-height: 35px;
    border-radius: 2px;
    height: 35px;
    width: 30%;
    margin: 0 2% 10px 0;
    text-align: center;
    background: #f2f2f2;
    display: inline-block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}


.sift-row .row-head label {
    font-size: 14px;
    color: #ff5000;
}


.selectCustomer
{
    width: 100%;
    height: 25px;
    line-height: 25px;
    border-radius: 5px;
    border: 1px solid #c4c7cc;
    padding: 0px 5px;
}

.show{
    animation: slideIn 400ms;
    -moz-animation: slideIn 400ms;	/* Firefox */
    -webkit-animation: slideIn 400ms;	/* Safari å’Œ Chrome */
    display: block;
}

@keyframes slideIn {
    from {height:0px;display: none;}
    to {height:100%;display: block}
}

@-webkit-keyframes slideIn {
    from {height:0px;display: none;}
    to {height:100%;display: block}
}

@keyframes slideOut {
    from {height:100%;display: block;}
    to {height:0px;display: none}
}

@-webkit-keyframes slideOut {
    from {height:100%;display: block;}
    to {height:0px;display: none}
}